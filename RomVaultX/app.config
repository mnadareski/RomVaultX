<?xml version="1.0" encoding="utf-8"?>
<configuration>
<appSettings>
	<!-- Database and Memory -->
	<add key="DBFileName" value="rom"/>
	<add key="DBMemCacheSize" value="8000000"/>
	<add key="ScanInMemorySize" value="1000000" />
	<add key="DBCheckOnStartup" value="false"/>
	<add key="SkipIndexingOnStartup" value="false" />
    <add key="RemoveMissingDats" value="true" />
  
	<!-- Virtual Drive -->
	<add key="vDriveLetter" value="P" />
  
	<!-- Paths -->
	<add key="DatRoot" value="DatRoot"/>
	<add key="ToSort" value="ToSort"/>
	<add key="Depot0" value="RomRoot" />
	<add key="Size0" value="-1" />  <!-- -1 is unlimited, anything else is a hard limit; unused -->
</appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup></configuration>
